{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "settings": {
    "react": {
      "version": "18"
    }
  },
  "rules": {
    "@next/next/no-html-link-for-pages": ["error", "packages/client-web/app"],
    "@typescript-eslint/no-explicit-any": "off",
    "no-console": ["error", { "allow": ["warn", "error", "info"] }],
    "no-debugger": "error"
  }
}
